<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Accounting.Core.Domain.Bank, Accounting.Core" table="Bank" >
    <id name="BankID" column="BankID" type="Int32" unsaved-value="0">
      <generator class="identity" />
    </id>
    <property name="BankCode" column="BankCode" type="string" length="255" />
    <property name="BankName" column="BankName" type="string" length="255" />
    <property name="BankNameEnglish" column="BankNameEnglish" type="string" length="255" />
    <property name="Address" column="Address" type="string" length="255" />
    <property name="Description" column="Description" type="string" length="255" />
    <property name="IsHasServiceOnline" column="IsHasServiceOnline" type="bool"/>
    <property name="Icon" column="Icon" type="BinaryBlob"/>
    <property name="Inactive" column="Inactive" type="bool"/>
  </class>
</hibernate-mapping>
